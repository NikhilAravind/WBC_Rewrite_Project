extends layout

block content
	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				#user
					h1.text-center.login-title Welcome #{user.firstName}. Please find the survey questions below:

						- var arr= states;
						
						each val in arr
							div.signup-wall
								p= val
							ul.user-details
								&nbsp
								&nbsp
								&nbsp
								&nbsp

						a(href='/signout', class='text-center new-account') Sign Out